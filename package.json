{
  "name": "Fontiny",
  "version": "1.0.0",
  "description": "font tiny by simmzl",
  "main": "./src/main/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
